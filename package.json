    "if else elseif",
    "conditional",
    "react condition"
  ],
  "author": "pyaesone17",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pyaesone17/react-else-if/issues"
  },
  "homepage": "https://github.com/pyaesone17/react-else-if#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.4.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "^3.1.2",
    "polished": "^1.3.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
