  "description": "React Conditional Renderer Component that allows you to write jsx like a template engine",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel lib -d build",
    "build:watch": "babel lib -w -d build",
    "lint": "eslint lib/**; exit 0",
